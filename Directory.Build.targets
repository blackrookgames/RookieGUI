  <!--Requires ScriptAssembliesDir to be defined-->
  <ItemGroup>
    <Reference Include="UnityEngine.UI">
      <HintPath>$(ScriptAssembliesDir)UnityEngine.UI.dll</HintPath>
    </Reference>
    <Reference Include="Unity.InputSystem">
      <HintPath>$(ScriptAssembliesDir)Unity.InputSystem.dll</HintPath>
    </Reference>
  </ItemGroup>
</Project>